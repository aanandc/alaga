<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sellsword.aanandchakravarthy.flinger">

    <application
        android:allowBackup="true"
        android:icon="@drawable/rocket"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen">
        <activity android:name=".FlingerActivity" android:screenOrientation="portrait"/>
        <activity android:name=".LoadingActivity" android:screenOrientation="portrait"/>
        <activity android:name=".StartScreenActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HighScoreActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>